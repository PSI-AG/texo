<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.eclipse.org/emf/texo/modelgenerator/annotations"
            class="org.eclipse.emf.texo.modelgenerator.modelannotations.ModelcodegeneratorPackage"
            genModel="model/texo-annotations-modelgenerator.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.emf.texo.extensionpoint.annotationmodels">
      <annotationModel
            class="org.eclipse.emf.texo.modelgenerator.modelannotations.ModelcodegeneratorPackage">
      </annotationModel>
   </extension>
   <extension
         point="org.eclipse.emf.texo.extensionpoint.modelannotators">
      <modelAnnotator
            class="org.eclipse.emf.texo.modelgenerator.annotator.ModelGenAnnotator">
      </modelAnnotator>
   </extension>

</plugin>
